{
	"name": "nature",
	"version": "1.0.0",
	"description": "landing page for nature",
	"main": "index.js",
	"scripts": {
		"watch:sass": "node-sass sass/main.scss css/style.css -w",
		"compile:sass": "node-sass sass/main.scss css/style.comp.css",
		"concat:css": "concat -o css/style.concat.css css/icon-fonts.css css/style.comp.css",
		"prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.concat.css -o css/style.prefix.css",
		"compress:css": "node-sass css/style.prefix.css css/style.css --outpust-style compressed",
		"build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Sanjula8/SASS-Example.git"
	},
	"author": "",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/Sanjula8/SASS-Example/issues"
	},
	"homepage": "https://github.com/Sanjula8/SASS-Example#readme",
	"devDependencies": {
		"autoprefixer": "^9.8.4",
		"concat": "^1.0.3",
		"node-sass": "^7.0.0",
		"npm-run-all": "^4.1.5",
		"postcss-cli": "^7.1.1"
	}
}
